{
  "@@locale": "en",
  "welcom_msg": "Hello, {name}!",
  "n_wombats": "Hello, {count, plural, =0{no wombats} =1{1 wombat} other{{count} wombats}}",
  "escap": "Hello! '{Isn''t}' this''s a wonderful day?",
  "dt_msg": "Hello, dt:{val}",
  "@dt_msg": {
    "placeholders": {
      "val": {
        "type": "DateTime",
        "format": "yyyy-MM-dd HH:mm:ss",
        "isCustomDateFormat": "true"
      }
    }
  },
  "num_msg": "Hello, num:{val}",
  "@num_msg": {
    "placeholders": {
      "val": {
        "type": "int",
        "format": "decimalPattern"
      }
    }
  },
  "str_msg": "Hello, str:{val}",
  "@str_msg": {
    "placeholders": {
      "val": {
        "type": "String"
      }
    }
  }
}